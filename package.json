    "debug": "babel-node  --presets @babel/env .\\src\\main.js",
    "babel-node": "babel-node --presets=@babel/env",
    "dev": "nodemon  --exec npm run babel-node -- .\\src\\main.js",
    "start": "node .\\dist\\index.js",
    "coverage": "SET NODE_ENV=test&& nyc mocha --timeout 5s --exit -s 20 --recursive "
  },
