  "main": "index.js",
  "nyc": {
    "check-coverage": true,
    "per-file": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "include": [
      "./src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary",
      "html"
    ],
    "require": [
      "@babel/register"
    ],
    "cache": true,
    "all": true,
    "temp-directory": "./alternative-tmp",
    "report-dir": "./alternative"
  },
  "scripts": {
    "test": "SET NODE_ENV=test&& mocha  --require @babel/register --timeout 5s --exit -s 20 --recursive ",
